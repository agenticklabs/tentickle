{
  "name": "tentickle",
  "version": "0.1.0",
  "description": "Autonomous coding agent built on agentick.",
  "keywords": [
    "agent",
    "agentick",
    "ai",
    "autonomous",
    "cli",
    "coding"
  ],
  "license": "MIT",
  "author": "Ryan Lindgren",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agenticklabs/tentickle.git",
    "directory": "packages/tentickle"
  },
  "bin": {
    "tentickle": "bin/tentickle.js"
  },
  "files": [
    "dist",
    "bin",
    "README.md"
  ],
  "type": "module",
  "main": "src/index.ts",
  "exports": {
    ".": "./src/index.ts"
  },
  "publishConfig": {
    "access": "public",
    "bin": {
      "tentickle": "bin/tentickle.js"
    },
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js"
      }
    },
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts"
  },
  "scripts": {
    "start": "node --env-file=../../.env --import tsx bin/tentickle.js",
    "start:coding": "node --env-file=../../.env --import tsx bin/tentickle.js --agent coding",
    "start:main": "node --env-file=../../.env --import tsx bin/tentickle.js --agent main",
    "build": "tsc -p tsconfig.build.json",
    "typecheck": "tsc -p tsconfig.build.json --noEmit",
    "clean": "rm -rf dist tsconfig.build.tsbuildinfo",
    "prepublishOnly": "pnpm build"
  },
  "dependencies": {
    "@tentickle/agent": "workspace:*",
    "@tentickle/cli": "workspace:*",
    "@tentickle/coding": "workspace:*",
    "@tentickle/main": "workspace:*",
    "@tentickle/tools": "workspace:*",
    "@tentickle/tui": "workspace:*"
  },
  "devDependencies": {
    "tsx": "^4.19.0"
  }
}
