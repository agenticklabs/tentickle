{
  "name": "@tentickle/tui",
  "version": "0.1.0",
  "description": "Shared TUI components for tentickle agents",
  "license": "MIT",
  "author": "Ryan Lindgren",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agenticklabs/tentickle.git",
    "directory": "packages/tui"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "src/index.ts",
  "exports": {
    ".": "./src/index.ts"
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js"
      }
    },
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts"
  },
  "scripts": {
    "build": "tsc -b tsconfig.build.json",
    "typecheck": "tsc --noEmit",
    "lint": "oxlint src/",
    "format:check": "oxfmt --check src/",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@agentick/client": "^0.9.0",
    "@agentick/core": "^0.9.0",
    "@agentick/devtools": "^0.9.0",
    "@agentick/gateway": "^0.9.0",
    "@agentick/react": "^0.9.0",
    "@agentick/scheduler": "^0.9.0",
    "@agentick/shared": "^0.9.0",
    "@agentick/tui": "^0.9.0",
    "@tentickle/agent": "workspace:*"
  },
  "devDependencies": {
    "@types/react": "^19.0.0",
    "ink": "^6.7.0",
    "react": "^19.0.0",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "react": "^19.0.0"
  }
}
