{
  "name": "@tentickle/cli",
  "version": "0.0.3",
  "description": "CLI entry point for tentickle agents",
  "license": "MIT",
  "author": "Ryan Lindgren",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agenticklabs/tentickle.git",
    "directory": "packages/cli"
  },
  "bin": {
    "tentickle": "bin/tentickle.ts"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "src/index.ts",
  "exports": {
    ".": "./src/index.ts"
  },
  "publishConfig": {
    "access": "public",
    "bin": {
      "tentickle": "dist/bin/tentickle.js"
    },
    "exports": {
      ".": {
        "types": "./dist/src/index.d.ts",
        "import": "./dist/src/index.js"
      }
    },
    "main": "./dist/src/index.js",
    "types": "./dist/src/index.d.ts"
  },
  "scripts": {
    "start": "node --env-file=../../.env --import tsx bin/tentickle.ts",
    "start:main": "node --env-file=../../.env --import tsx bin/tentickle.ts",
    "start:coding": "node --env-file=../../.env --import tsx bin/tentickle.ts --agent coding",
    "build": "tsc -b tsconfig.build.json",
    "typecheck": "tsc --noEmit",
    "lint": "oxlint src/ bin/",
    "format:check": "oxfmt --check src/ bin/",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@agentick/client": "^0.8.0",
    "@agentick/connector-telegram": "*",
    "@agentick/devtools": "^0.8.0",
    "@agentick/gateway": "^0.8.0",
    "@agentick/scheduler": "^0.7.1",
    "@tentickle/agent": "workspace:*",
    "@tentickle/coding": "workspace:*",
    "@tentickle/main": "workspace:*",
    "@tentickle/tui": "workspace:*",
    "commander": "^14.0.3"
  },
  "devDependencies": {
    "tsx": "^4.19.0",
    "typescript": "^5.9.3"
  }
}
