{
  "name": "@tentickle/coding",
  "version": "0.0.2",
  "description": "Coding agent â€” autonomous software engineering",
  "license": "MIT",
  "author": "Ryan Lindgren",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agenticklabs/tentickle.git",
    "directory": "agents/coding"
  },
  "bin": {
    "tentickle": "bin/tentickle.ts"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "src/index.ts",
  "exports": {
    ".": "./src/index.ts"
  },
  "publishConfig": {
    "access": "public",
    "bin": {
      "tentickle": "dist/bin/tentickle.js"
    },
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js"
      }
    },
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts"
  },
  "scripts": {
    "start": "node --env-file=../../.env --import tsx bin/tentickle.ts",
    "dev": "node --env-file=../../.env --import tsx --watch bin/tentickle.ts",
    "build": "tsc -b tsconfig.build.json",
    "typecheck": "tsc --noEmit",
    "lint": "oxlint src/",
    "format:check": "oxfmt --check src/",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@agentick/client": "*",
    "@agentick/core": "*",
    "@agentick/devtools": "*",
    "@agentick/react": "*",
    "@agentick/scheduler": "*",
    "@agentick/shared": "*",
    "@agentick/tui": "*",
    "@tentickle/agent": "workspace:*",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/react": "^19.0.0",
    "ink": "^5.1.0",
    "ink-text-input": "^6.0.0",
    "react": "^19.0.0",
    "tsx": "^4.19.0",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "react": "^19.0.0"
  }
}
